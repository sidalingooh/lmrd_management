<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>

    <!--<classPathEntry
            location="D:\workJAR\mysql-connector-java-5.1.38.jar" />-->
    <!-- <classPathEntry location="D:\jar\postgresql.jar" /> -->


    <context id="DB2Tables" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="utf-8" />

        <commentGenerator type="com.lmrd.util.MyCommentGenerator">
            <property name="suppressDate" value="true" />
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false" />
        </commentGenerator>

        <!-- 数据库链接 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/lmrd?characterEncoding=utf8"
                        userId="root" password="MySQLRoot123">

            <property name="serverTimezone" value="UTC" />
            <property name="useInformationSchema" value="true" />
            <property name="useSSL" value="false" />
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- model 包路径 -->
        <javaModelGenerator targetPackage="com.lmrd.entity.industryClassification"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- dao xml(sql) 包路径 -->
        <sqlMapGenerator targetPackage="config.mybatis.mapper.industryClassification"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!-- dao 包路径 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.lmrd.dao.industryClassification" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!-- tableName：表名 domainObjectName:实体类名字，dao类会在此基础上加上mapper -->

